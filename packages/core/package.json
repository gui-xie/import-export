{
  "name": "@senlinz/import-export",
  "version": "0.1.0-beta.13",
  "description": "Stencil Component for import/export excel files.",
  "module": "dist/components/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts",
    "README.md"
  ],
  "exports": {
    ".": {
      "import": "./dist/components/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec",
    "e2e-serve": "stencil build --config stencil.test.config.ts && serve -l 8080 ./tests",
    "e2e-test": "playwright test",
    "generate": "stencil generate",
    "publish-dry-run": "pnpm publish --dry-run --no-git-checks"
  },
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "@stencil/core": "^4.7.0",
    "@types/jest": "^29.5.6",
    "@types/node": "^16.18.108",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0"
  },
  "author": "senlin",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gui-xie/import-export.git"
  },
  "dependencies": {
    "@senlinz/import-export-wasm": "workspace:*",
    "fflate": "^0.8.2"
  },
  "keywords": [
    "stencil",
    "webcomponents",
    "rust",
    "wasm",
    "import",
    "export",
    "excel",
    "xlsx",
    "sheet",
    "spreadsheet",
    "OpenXML",
    "browser",
    "client"
  ]
}